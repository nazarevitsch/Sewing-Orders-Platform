{
  "compilerOptions": {
    "typeRoots": ["node_modules/@types"],
    "outDir": "built",
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  },
  "include": [
    "src"
  ],
  "exclude":[
    "node_modules"
  ]
}
